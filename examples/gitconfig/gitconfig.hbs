[user]
    email = <{{ user.email }}>
    name = <{{ user.name }}>
[alias]
    lg = !git lg1
    lg1 = !git lg1-specific --all
    lg2 = !git lg2-specific --all
    lg3 = !git lg3-specific --all
[pager]
    branch = false
[core]
	editor = vim
[pull]
	rebase = false
[init]
	defaultBranch = main
[fetch]
	prune = true
